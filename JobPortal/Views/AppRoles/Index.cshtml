@{
    ViewData["Title"] = "Application Roles";
}
<h1>View Roles</h1>
<p>
    <a asp-action="Create"> Create Role</a>
</p>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
            </tr>
        }

    </tbody>
</table>